{
  "name": "parse-authkit",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "publish:next": "cd packages/next && pnpm publish",
    "publish:auth-adapters": "cd packages/auth-adapters && pnpm publish",
    "publish:parse": "cd packages/parse && pnpm publish",
    "publish:all": "pnpm build && pnpm publish:next && pnpm publish:auth-adapters && pnpm publish:parse"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
